pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'maven:3.8.1-adoptopenjdk-11' }
      }
      steps {
        sh 'mvn --version'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'nginx:1.25.1-alpine-slim' }
      }
      steps {
        sh 'nginx --v'
      }
    }
  }
}
