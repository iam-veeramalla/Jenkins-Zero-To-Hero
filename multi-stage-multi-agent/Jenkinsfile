pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'maven:3.8.1-adoptopenjdk-11' }
      }
      steps {
        sh 'mvn --version'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'node --version'
      }
    }
    stage('docker login') {
      steps {
        scripts {
          sh "docker login -u gowda0921 -p Dai@2023!"
        }
      }
    }
    stage('DB') {
      agent {
        docker { image 'mys:16-alpine'  }
      }
      steps { 
        sh 'node --version'
      }
    }
}
